{% extends "base.html.twig" %}
{% block title %}
Les chatons de dossier {{ dossier }}
{% endblock %}
{% block body %}
    <h1>Les chatons du dossier <i>{{ dossier }}</i></h1>
    <div class="row">
    {% for chaton in photos %}
        <div class="col col-3">
            <div class="card">
                <img src="{{ asset("/photos/" ~ dossier ~ "/" ~ chaton.filename) }}">
                {{ chaton.filename }}<br>
            </div>
        </div>
    {% endfor %}
    </div>
{% endblock %}